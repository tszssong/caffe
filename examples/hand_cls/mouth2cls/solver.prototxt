net: "examples/hand_cls/mouth2cls/traintest.prototxt"
base_lr: 0.01
momentum: 0.9
momentum2: 0.999

type: "SGD"
#type: "Adam"
#lr_policy: "fixed"
test_iter: 1
test_interval: 1000
weight_decay: 0.004
lr_policy: "step"
stepsize: 20000
gamma: 0.8
display: 50
max_iter: 1000000
snapshot: 5000
snapshot_prefix: "models/mouth2cls/600w"
solver_mode: CPU
