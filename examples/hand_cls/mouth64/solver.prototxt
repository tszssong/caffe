net: "examples/hand_cls/mouth64/traintest.prototxt"
base_lr: 0.001
momentum: 0.9
momentum2: 0.999

type: "SGD"
#type: "Adam"
#lr_policy: "fixed"
test_iter: 1
test_interval: 1000
weight_decay: 0.004
lr_policy: "step"
stepsize: 100000
gamma: 0.8
display: 50
max_iter: 100000
snapshot: 5000
snapshot_prefix: "models/mouth64/dw"
solver_mode: CPU
